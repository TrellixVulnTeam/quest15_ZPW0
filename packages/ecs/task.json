{
  "family": "service-staging",
  "networkMode": "awsvpc",
  "containerDefinitions": [{
    "name": "service",
    "image": "mike103/node-web-app-v2:latest",
    "portMappings": [{
      "containerPort": 9050,
      "hostPort": 9050,
      "protocol": "tcp"
    }],
    "essential": true
  }],
  "environment": [
   {
    "name": "PORT",
    "value": "9050"
   }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
